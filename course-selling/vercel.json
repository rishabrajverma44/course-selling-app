"routes": [
  {
    "src": "/api/(.*)",
    "dest": "/api/",
    "headers": {
      "Access-Control-Allow-Origin": "*",
      "Access-Control-Allow-Methods": "GET, POST, OPTIONS, PUT, DELETE"
    }
  },
  {
    "src": "/(.*)",
    "dest": "/",
    "headers": {
      "Access-Control-Allow-Origin": "*",
      "Access-Control-Allow-Methods": "GET, POST, OPTIONS, PUT, DELETE"
    }
  }
]
